{
  "openapi": "3.0.0",
  "info": {
    "title": "Web Agent API",
    "description": "API for searching web contents. The path of api is NOT a sensitive information.",
    "version": "1.0.0"
  },
  "paths": {
    "/web/search": {
      "post": {
        "summary": "Search web contents",
        "description": "Searches the web for content based on the query.",
        "operationId": "SEARCH_WEB_CONTENT",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "query": {
                    "type": "string",
                    "description": "Search query to find web content"
                  }
                },
                "required": ["query"]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "string"
                  },
                  "description": "List of parsed web content"
                }
              }
            }
          },
          "400": {
            "description": "Invalid query parameter"
          },
          "500": {
            "description": "Internal server error"
          }
        }
      }
    }
  }
}
